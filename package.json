{
  "name": "omardiab.com",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:fab": "npm run build && npm run fab:build",
    "fab:build": "fab build --minify",
    "fab:serve": "fab serve fab.zip",
    "deploy": "yarn build:fab && fab deploy",
    "tail": "dotenv wrangler tail",
    "start": "next start",
    "eslint": "eslint --ignore-path .gitignore --ext \"js,ts,jsx,tsx\"",
    "eslint:src": "yarn eslint ./src",
    "prettier:src": "prettier \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "yarn prettier:src --check && yarn eslint:src",
    "lint:fix": "yarn prettier:src --write && yarn eslint:src --fix",
    "test": "yarn lint && tsc"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.1.4",
    "@sentry/browser": "^6.2.3",
    "@sentry/integrations": "^6.2.3",
    "@sentry/node": "^6.2.3",
    "@sentry/webpack-plugin": "^1.14.2",
    "file-loader": "^6.2.0",
    "next": "^10.0.5",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@cloudflare/wrangler": "^1.13.0",
    "@emotion/babel-plugin": "^11.1.2",
    "@emotion/babel-preset-css-prop": "^11.0.0",
    "@fab/actions": "^1.0.0-rc.9",
    "@fab/cli": "^1.0.0-rc.9",
    "@fab/deployer-cf-workers": "^1.0.0-rc.9",
    "@fab/input-nextjs": "^1.0.0-rc.9",
    "@fab/plugin-render-html": "^1.0.0-rc.9",
    "@fab/plugin-rewire-assets": "^1.0.0-rc.9",
    "@fab/server": "^1.0.0-rc.9.p1",
    "@next/env": "^10.0.7",
    "@svgr/webpack": "^5.5.0",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.55",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-react": "^7.21.5",
    "prettier": "^2.1.2",
    "typescript": "^4.2.4",
    "webpack-merge": "^5.7.3"
  },
