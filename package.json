{
  "name": "omardiab.com",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "dev": "doppler run -- astro dev",
    "start": "deno run --allow-net --allow-read --allow-env ./dist/server/entry.mjs",
    "tail": "dotenv wrangler tail",
    "build": "astro build",
    "eslint": "eslint --ignore-path .gitignore --ext \"js,ts,jsx,tsx,astro\"",
    "eslint:src": "yarn eslint ./src",
    "prettier:src": "prettier \"src/**/*.{ts,tsx,js,jsx}\"",
    "typecheck": "yarn tsc",
    "lint": "yarn prettier:src --check && yarn eslint:src",
    "lint:fix": "yarn prettier:src --write && yarn eslint:src --fix",
    "format": "yarn prettier:src --write",
    "test": "yarn lint && tsc"
  },
  "devDependencies": {
    "@astrojs/deno": "^1.0.1",
    "@astrojs/mdx": "^0.11.1",
    "@astrojs/preact": "^1.1.0",
    "@astrojs/react": "^1.1.2",
    "@types/body-parser": "^1.19.2",
    "@types/cookie": "^0.5.1",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "^18.7.16",
    "@types/react": "^18.0.21",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vanilla-extract/css": "^1.9.0",
    "@vanilla-extract/css-utils": "^0.1.2",
    "@vanilla-extract/recipes": "^0.2.5",
    "@vanilla-extract/vite-plugin": "^3.5.0",
    "astro": "^1.2.1",
    "autoprefixer": "^10.4.8",
    "classnames": "^2.3.2",
    "cookie": "^0.5.0",
    "cssnano": "^5.1.13",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-astro": "^0.19.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "globby": "^13.1.2",
    "is-absolute-url": "^4.0.1",
    "preact": "^10.6.5",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.5.4",
    "react": "npm:@preact/compat@^17",
    "react-dom": "npm:@preact/compat@^17",
    "react-icons": "^4.4.0",
    "sass": "^1.54.9",
    "type-fest": "^2.19.0",
    "typescript": "^4.8.3",
    "url": "^0.11.0",
    "web-streams-polyfill": "^3.2.1",
    "zod": "^3.19.1"
  },
  "license": "MIT",
  "packageManager": "yarn@3.2.3"
}
